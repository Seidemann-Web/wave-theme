{
    "name": "seidemann-web/wave-theme",
    "description": "This is Wave Theme for OXID eShop.",
    "type": "oxideshop-theme",
    "keywords": [
        "oxid",
        "themes",
        "eShop"
    ],
    "homepage": "https://www.oxid-esales.com/en/home.html",
    "license": [
        "GPL-3.0-only",
        "proprietary"
    ],
    "minimum-stability": "dev",
    "replace": {
        "oxid-esales/wave-theme": "self.version"
    },
    "extra": {
        "oxideshop": {
            "target-directory": "wave",
            "assets-directory": "out/wave",
            "blacklist-filter": [
                ".gitignore",
                ".gitattributes",
                "CHANGELOG.md",
                "CONTRIBUTING.md"
            ]
        }
    }
}
